{
  "name": "overseas-web",
  "version": "0.0.1",
  "description": "Use vue3 vite eslint SSR vuex vue-router element-plus scss",
  "author": "AAGame",
  "watchFile": "./src/assets/colors/color.js",
  "outputDir": "./src/assets/colors",
  "scripts": {
    "watch": "watchColor",
    "start": "node scripts/cli.mjs",
    "sever": "vite --host --mode development",
    "favicon": "cross-env NODE_ENV=production node scripts/favicon",
    "build": "node scripts/build",
    "build:test": "node scripts/buildTest.mjs",
    "build:spa": "vite build",
    "test": "cross-env SOURCE_MAP=true npm run build && cross-env NODE_ENV=test node --expose-gc server",
    "translate": "node scripts/translate.mjs",
    "lint": "eslint . --fix --ext .js,.mjs,.ts,.vue --ignore-path .gitignore",
    "postinstall": "patch-package",
    "re": "rm -rf node_modules/.vite && npm run dev",
    "prepare": "husky install",
    "init": "node scripts/init",
    "replace": "node scripts/autoGit.js",
    "tpl": "node scripts/submodule"
  },
  "dependencies": {
    "ahooks-vue": "^0.12.9",
    "axios": "^0.27.2",
    "dayjs": "^1.11.1",
    "dom-to-image": "^2.6.0",
    "echarts": "^5.3.2",
    "element-plus": "1.0.2-beta.55",
    "file-saver": "^2.0.5",
    "inquirer": "^8.2.4",
    "js-cookie": "^3.0.1",
    "lru-cache": "^7.9.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "once-init": "^0.8.1",
    "qrcode": "^1.5.0",
    "shelljs": "^0.8.5",
    "socket.io-client": "^2.3.0",
    "swiper": "^8.1.4",
    "video.js": "^7.18.1",
    "vue": "^3.2.33",
    "vue-i18n": "^9.1.10",
    "vue-list-marquee": "^1.0.48",
    "vue-router": "^4.0.14",
    "vue3-clipboard": "^1.0.0",
    "vue3-colorpicker": "^1.0.5",
    "vue3-count-to": "^1.1.2",
    "vue3-draggable-resizable": "1.6.3",
    "vue3-lazyload": "0.2.5-beta",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@aagames-fe/google-translate": "^0.0.12",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@google-cloud/translate": "^6.3.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-replace": "^4.0.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/server-renderer": "^3.2.33",
    "consola": "^2.15.3",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^8.7.1",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.1",
    "gogocode": "^1.0.53",
    "google-translate-api": "^2.3.0",
    "google-translate-token": "^1.0.0",
    "got": "^12.0.4",
    "husky": "^7.0.4",
    "md5": "^2.3.0",
    "patch-package": "^6.4.7",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-visualizer": "^5.6.0",
