    "tinify": "^1.6.1",
    "unplugin-auto-import": "^0.7.1",
    "vite": "2.9.5",
    "vite-plugin-eslint": "^1.6.0",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "2.0.1",
    "watch-color": "^1.0.9"
  },
  "resolutions": {
    "console": "consola"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vok123/vue3-ts-vite-ssr-starter.git"
